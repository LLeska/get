import RPi.GPIO as GPIO
import time

#const
led_pin = 26

#setup
GPIO.setwarnings(False)

GPIO.setmode(GPIO.BCM)
GPIO.setup(led_pin, GPIO.OUT)


#var
pwm = GPIO.PWM(led_pin, 200)
duty = 0.0


#main loop
pwm.start(duty)
while True:
    pwm.ChangeDutyCycle(duty)
    time.sleep(0.05)

    duty += 1.0
    if duty > 100.0: duty = 0.0

GPIO.cleanup()
